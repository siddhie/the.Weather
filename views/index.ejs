<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="css/styles.css" />
    <link
      rel="stylesheet"
      href="https://unicons.iconscout.com/release/v4.0.0/css/line.css"
    />

    <!-- Bootstrap link -->
    <!-- CSS only -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor"
      crossorigin="anonymous"
    />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <title>Weather</title>
  </head>
  <body class="full__cover">
    <!-- image -->

    <!-- Header  -->
    <main class="font__color_dark">
      <!-- navbar -->
      <header>
        <nav class="nav" id="nav">
          <div class="nav__part-1 nav__name">the.Weather</div>

          <div class="nav__part-2">
            <a
              class="nav__contactus"
              target="_blank"
              href="mailto:the.Weather.contact@gmail.com"
              >Contact Us
            </a>

            <div class="themechange__btn">
              <a><i class="uil uil-moon weather__icons themeChangeIcon"></i></a>
            </div>
          </div>
        </nav>
      </header>

      <!-- ======================= search section ============ -->
      <!-- search box -->
      <form action="" method="post" id="form-post">
        <div class="search__box">
          <!-- bootstrap search code  starts  -->

          <section class="intro">
            <div class="bg-image h-100">
              <div class="mask d-flex align-items-center h-100">
                <div class="container">
                  <div class="row">
                    <div class="col-md-10 col-lg-8 col-xl-7 mx-auto">
                      <div
                        class="card mb-2"
                        style="border-radius: 10rem; border: none; height: 60px"
                      >
                        <div class="card-body p-2">
                          <div class="input-group input-group-lg">
                            <input
                              type="text"
                              class="form-control form-control-lg rounde bg-transparent text-white input_area"
                              placeholder="Search for a city"
                              aria-label="Type Keywords"
                              aria-describedby="basic-addon2"
                              name="city"
                            />
                            <span
                              class="input-group-text border-0"
                              id="basic-addon2"
                              ><a
                                class="dark__search"
                                href="javascript:$('form-post').submit();"
                                onclick="document.getElementById('form-post').submit();"
                                ><i class="uil uil-search text-blue"></i></a
                            ></span>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </section>

          <!-- bootstrap search code ends -->
        </div>
      </form>

      <!-- section weather -->
      <section class="weather__data">
        <div class="weather">
          <!-- 1. temp -->
          <div class="temperature">
            <span class="temp"> <%=temp%> </span>
            <span class="circle"> &#0176; </span>
            <span class="celcius"> c </span>
          </div>

          <!-- 2. location icon -->
          <div class="location__icon">
            <div class="location__name"><%=cityName%></div>

            <div class="weather__icon">
              <img
                src="https://openweathermap.org/img/wn/<%=iconId%>@2x.png"
                alt="image"
                class="icon"
              />
            </div>
          </div>
          <!-- /*location icon ends*// -->
        </div>
        <!-- weather div ends  -->

        <!-- image ends -->

        <div class="weather__info">
          <div class="snowflake flex_temp">
            <a href=""><i class="uil uil-snowflake weather__icons"></i></a>
            <span class="side__data"><%=humidity%> %</span>
          </div>

          <div class="wind flex_temp">
            <a href=""><i class="uil uil-wind weather__icons"></i></a>
            <span class="side__data"><%=windSpeed%> km/h</span>
          </div>
          <div class="temp_min flex_temp">
            <a href=""
              ><i class="uil uil-temperature-empty weather__icons"></i
            ></a>
            <span class="side__data"><%=tempMin%> &#0176;</span>
          </div>
          <div class="temp_max flex_temp">
            <a href=""
              ><i class="uil uil-temperature-three-quarter weather__icons"></i
            ></a>
            <span class="side__data"><%=tempMax%> &#0176;</span>
          </div>
        </div>
        <!-- weather  info div ends -->
      </section>

      <!-- section description -->
      <section class="description__section">
        <!-- background image starts -->

        <div class="background__image">
          <!-- try -->
          <style>
            .animated {
              width: 450px;
              height: 450px;
            }
            svg#freepik_stories-fall-is-coming:not(.animated) .animable {
              opacity: 0;
            }
            svg#freepik_stories-fall-is-coming.animated
              #freepik--Character--inject-55 {
              animation: 1s 1 forwards cubic-bezier(0.36, -0.01, 0.5, 1.38)
                  slideUp,
                1.5s Infinite linear floating;
              animation-delay: 0s, 1s;
            }
            svg#freepik_stories-fall-is-coming.animated
              #freepik--Leafs--inject-55 {
              animation: 1s 1 forwards cubic-bezier(0.36, -0.01, 0.5, 1.38)
                  fadeIn,
                1.5s Infinite linear wind;
              animation-delay: 0s, 1s;
            }
            @keyframes slideUp {
              0% {
                opacity: 0;
                transform: translateY(30px);
              }
              100% {
                opacity: 1;
                transform: inherit;
              }
            }
            @keyframes floating {
              0% {
                opacity: 1;
                transform: translateY(0px);
              }
              50% {
                transform: translateY(-10px);
              }
              100% {
                opacity: 1;
                transform: translateY(0px);
              }
            }
            @keyframes fadeIn {
              0% {
                opacity: 0;
              }
              100% {
                opacity: 1;
              }
            }
            @keyframes wind {
              0% {
                transform: rotate(0deg);
              }
              25% {
                transform: rotate(1deg);
              }
              75% {
                transform: rotate(-1deg);
              }
            }
            @media screen and (max-width: 1350px) {
              .animated {
                width: 400px;
                height: 400px;
              }
            }
            @media screen and (max-width: 1280px) {
              .animated {
                width: 350px;
                height: 350px;
              }
            }
            @media screen and (max-width: 950px) {
              .animated {
                width: 270px;
                height: 270px;
              }
            }
            @media screen and (max-width: 769px) {
              .animated {
                width: 300px;
                height: 300px;
              }
            }
